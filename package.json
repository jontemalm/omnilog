{
  "name": "omnilog",
  "version": "1.1.0",
  "author": "Jonas Malm",
  "displayName": "OmniLog",
  "main": "./src/electron.prod.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jontemalm/omnilog.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron:dev\"",
    "start-server": "node ./server.js",
    "serve": "npm run start-server && npm start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:dev": "electron ./app/electron.dev",
    "electron": "electron ./app/electron.prod",
    "package": "electron-builder -c",
    "electron:build": "npm run build && electron-builder build --publish=always -c",
    "package:win2": "cross-var electron-packager ./ $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite ",
    "package:win": "npm run build && ncp package.json ./dist/log-client/package.json && ncp ./node_modules ./dist/log-client/node_modules \"dist/log-client/electron.prod.js\" && cross-var electron-packager dist/log-client $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite --debug",
    "package:linux": "npm run build && cross-var electron-packager dist/log-client $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
    "package:osx": "npm run build && cross-var electron-packager dist/log-client $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "package:all": "npm run build && cross-var electron-packager dist/log-client $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "dgram": "^1.0.1",
    "dns": "^0.2.2",
    "electron-log": "^4.4.6",
    "electron-preferences": "^2.5.0",
    "electron-updater": "^4.6.5",
    "fast-xml-parser": "^4.0.0-beta.8",
    "ncp": "^2.0.0",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "rxjs": "~6.6.0",
    "ws": "^8.4.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.17",
    "@angular/cli": "~11.2.17",
    "@angular/compiler-cli": "~11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/ws": "^8.2.2",
    "codelyzer": "^6.0.0",
    "concurrently": "^7.0.0",
    "cross-var": "^1.1.0",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "electron-packager": "^15.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
